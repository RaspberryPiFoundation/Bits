{
  "name": "raspberry-pi-bits",
  "description": "The Raspberry Pi Front End Pattern Library",
  "author": {
    "name": "The Raspberry Pi Foundation",
    "url": "https://raspberrypi.org"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/RaspberryPiFoundation/Bits.git"
  },
  "version": "1.1.19",
  "main": "lib/index.js",
  "files": [
    "lib",
    "src/components",
    "src/styles"
  ],
  "scripts": {
    "build": "npm-run-all rollup gulp:compile_all",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "fix": "eslint src/. --ext .js --fix; exit 0;",
    "gulp:compile_all": "gulp compile_all",
    "gulp:watch": "gulp watch",
    "lint": "eslint src/. --ext .js; exit 0;",
    "lint:staged": "lint-staged",
    "prepublishOnly": "npm run build",
    "rollup": "rollup -c",
    "rollup:watch": "rollup -c -w",
    "start": "npm-run-all rollup:watch gulp:watch",
    "test": "jest --env=jsdom --notify --watch",
    "test-coverage": "jest --env=jsdom --coverage"
  },
  "peerDependencies": {
    "classnames": "^2.2.5",
    "prop-types": "^15.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^24.8.0",
    "css-loader": "^0.28.11",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.1",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.10.0",
    "eslint-plugin-jest": "21.3.2",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-prettier": "^2.7.0",
    "eslint-plugin-react": "^7.13.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-cssnano": "^2.1.3",
    "gulp-header": "^2.0.7",
    "gulp-notify": "^3.2.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^3.2.1",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-strip-css-comments": "^2.0.0",
    "jest": "^24.8.0",
    "jest-css-modules": "^2.0.0",
    "jest-environment-enzyme": "^7.0.2",
    "jest-enzyme": "^7.0.2",
    "lint-staged": "5.0.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.1",
    "prettier-package-json": "^1.6.0",
    "raf": "^3.4.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^4.3.1",
    "react-test-renderer": "^16.8.6",
    "rollup": "^1.12.3",
    "rollup-plugin-babel": "^4.0.1",
    "rollup-plugin-commonjs": "^8.4.1",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-sass": "^0.5.3"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./src/setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sss|styl)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "testEnvironment": "enzyme"
  },
  "jsnext:main": "lib/index.es.js",
  "lint-staged": {
    "package.json": [
      "prettier-package-json --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "module": "lib/index.es.js",
  "pre-commit": "lint:staged",
  "dependencies": {
    "iotacss": "^1.6.0",
    "react-iotacss": "^1.3.1"
  }
}
